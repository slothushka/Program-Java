import java.util.*;
class Main{
    public static void main(String []args){
        Scanner in= new Scanner(System.in);
        System.out.println("Enter the two number x and y : ");
        int x=in.nextInt();
        int y=in.nextInt();
     
     int sum=x+y;
     System.out.println("The sum of two numbers is "+sum);
        
    }
}
